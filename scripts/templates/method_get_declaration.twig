{% if '[]' in field.type %}
{% if field.allowed_getters %}
{% for getter in field.allowed_getters %}
{% if getter == 'vector' %}
                /**
                * @brief Get {{field.name}}
                * @return {{field.type}}
                */
                std::vector<uint8_t> get{{field.name_camel}}();
{% elseif getter == 'string' %}
                /**
                * @brief Get {{field.name}}
                * @return std::string
                */
                std::string get{{field.name_camel}}();
{% endif %}
{% endfor %}
{% else %}
                /**
                * @brief Get {{field.name}}
                * @return std::vector<uint8_t>
                */
                std::vector<uint8_t> get{{field.name_camel}}();
{% endif %}
{% elseif field.type == 'variable' %}
{% if field.allowed_getters %}
{% for getter in field.allowed_getters %}
{% if getter == 'vector' %}
                /**
                * @brief Get {{field.name}}
                * @return std::vector<uint8_t>
                */
                std::vector<uint8_t> get{{field.name_camel}}();
{% elseif getter == 'string' %}
                /**
                * @brief Get {{field.name}}
                * @return std::string
                */
                std::string get{{field.name_camel}}();
{% endif %}
{% endfor %}
{% else %}
                /**
                * @brief Get {{field.name}}
                * @return std::vector<uint8_t>
                */
                std::vector<uint8_t> get{{field.name_camel}}();
{% endif %}
{% else %}
                /**
                * @brief Get {{field.name}}
                * @return {{field.type}}
                */
                {{field.type}} get{{field.name_camel}}();
{% endif %}
